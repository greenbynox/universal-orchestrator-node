{
  "name": "node-orchestrator",
  "version": "1.0.0",
  "description": "MVP Orchestrateur de Nodes Multi-Blockchains (BTC, ETH, SOL, XMR, BNB)",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "ts-node-dev --respawn src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "tsc && cd frontend && npm run build",
    "start": "node dist/server.js",
    "test": "jest",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "blockchain",
    "node",
    "orchestrator",
    "bitcoin",
    "ethereum",
    "solana",
    "monero",
    "bnb",
    "crypto",
    "wallet"
  ],
  "author": "Node Orchestrator Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bip32": "^5.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "cors": "^2.8.5",
    "dockerode": "^4.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.2",
    "systeminformation": "^5.21.20",
    "tiny-secp256k1": "^2.2.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bip32": "^1.0.2",
    "@types/bitcoinjs-lib": "^4.0.1",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.23",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/helmet": "^0.0.48",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/systeminformation": "^3.23.1",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
