{
  "name": "node-orchestrator",
  "version": "2.3.2",
  "build": {
    "appId": "com.node.orchestrator",
    "productName": "Node Orchestrator",
    "copyright": "Copyright © 2026 Node Orchestrator Team",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json",
      "node_modules/**/*"
    ],
    "win": {
      "target": "portable",
      "icon": "electron/assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "icon": "electron/assets/icon.png"
    },
    "mac": {
      "target": "zip",
      "icon": "electron/assets/icon.icns"
    },
    "publish": null
  },
  "description": "Orchestrateur de Nodes Multi-Blockchains – Refonte Architecturale Data-Driven (77 Blockchain supportées sur 227)",
  "main": "electron/main.js",
  "scripts": {
    "docker:start": "powershell -ExecutionPolicy Bypass -File scripts/start-docker.ps1",
    "dev": "powershell -ExecutionPolicy Bypass -File scripts/dev.ps1",
    "dev:backend": "ts-node-dev --respawn src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:concurrent": "concurrently \"npm run dev:backend\" \"powershell -NoProfile -ExecutionPolicy Bypass -Command \\\"node scripts/wait-for-backend.js; if($LASTEXITCODE -ne 0){ exit $LASTEXITCODE }; Set-Location frontend; npm run dev\\\"\"",
    "start:dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run electron:dev\"",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "start": "node dist/server.js",
    "electron": "electron .",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux",
    "dist:all": "npm run build && electron-builder -mwl",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts --ignore-path .eslintignore",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:cleanup": "powershell -ExecutionPolicy Bypass -File cleanup_docker.ps1",
    "docker:wipe": "powershell -ExecutionPolicy Bypass -File cleanup_docker.ps1 -WipeAll",
    "docker:wipe:force": "powershell -ExecutionPolicy Bypass -File cleanup_docker.ps1 -WipeAll -Force",
    "generate:icons": "node scripts/generate-icons.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "keywords": [
    "blockchain",
    "node",
    "orchestrator",
    "bitcoin",
    "ethereum",
    "solana",
    "monero",
    "bnb",
    "crypto",
    "wallet"
  ],
  "author": {
    "name": "Node Orchestrator Team",
    "email": "team@greenbynox.com"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.6.2",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "cors": "^2.8.5",
    "dockerode": "^4.0.0",
    "dotenv": "^16.3.1",
    "ed25519-hd-key": "^1.3.0",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.2",
    "systeminformation": "^5.21.20",
    "tiny-secp256k1": "^2.2.4",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "optionalDependencies": {
    "@ledgerhq/hw-transport-node-hid": "^6.28.2",
    "@trezor/connect": "^9.2.1"
  },
  "devDependencies": {
    "@types/bip32": "^1.0.2",
    "@types/bitcoinjs-lib": "^4.0.1",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.23",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/systeminformation": "^3.23.1",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "app-builder-bin": "^4.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^39.2.5",
    "electron-builder": "^26.0.12",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^30.2.0",
    "pkg": "^5.8.1",
    "prisma": "^5.8.1",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "node-gyp": "^10.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}